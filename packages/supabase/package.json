{
    "name": "@makify/supabase",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./src/index.ts",
            "default": "./dist/index.js"
        },
        "./server": {
            "types": "./dist/client/server.d.ts",
            "import": "./src/client/server.ts",
            "default": "./dist/client/server.js"
        },
        "./client": {
            "types": "./dist/client/client.d.ts",
            "import": "./src/client/client.ts",
            "default": "./dist/client/client.js"
        },
        "./middleware": {
            "types": "./dist/middleware.d.ts",
            "import": "./src/middleware.ts",
            "default": "./dist/middleware.js"
        }
    },
    "typesVersions": {
        "*": {
            "server": [
                "./src/client/server.ts"
            ],
            "client": [
                "./src/client/client.ts"
            ],
            "middleware": [
                "./src/middleware.ts"
            ],
            "*": [
                "./src/index.ts"
            ]
        }
    },
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "lint": "eslint src/",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@supabase/ssr": "0.5.1"
    },
    "devDependencies": {
        "@supabase/supabase-js": "^2.39.0",
        "@types/node": "^20.0.0",
        "tsup": "8.0.0",
        "typescript": "5.3.3"
    }
}